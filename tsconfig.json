// PEGUEI AS CONFIGS DE TS DO BACKOFFICE DA LANCE PORQUE ESSE ANTIGO TAVA DANDO ALGUNS ERROS, QUALQUER COISA VOLTAMOS DEPOIS

{
  "compilerOptions": {
    "lib": ["es2017"],
    "skipLibCheck": true,
    "moduleResolution": "node",
    "noUnusedLocals": false,
    "noUnusedParameters": false,
    "sourceMap": true,
    "target": "es2017",
    "outDir": "dist",
    "rootDir": "./",
    "module": "commonjs",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true
    // "strict": true
  },
  "exclude": ["node_modules"]
}
